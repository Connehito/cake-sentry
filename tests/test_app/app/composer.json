{
    "name": "connehito/cake-sentry-test-app",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "/dist"
        }
    ],
    "require": {
        "php": "^7.1",
        "cakephp/cakephp": "^3.6",
        "cakephp/plugin-installer": "^1.0",
        "connehito/cake-sentry": "@dev"
    },
    "require-dev": {
        "cakephp/bake": "@stable",
        "cakephp/debug_kit": "@stable",
        "psy/psysh": "@stable"
    },
    "authors": [
        {
            "name": "Hideki Kinjyo",
            "email": "dev@o0h.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "config": {
        "sort-packages": true
    }
}
